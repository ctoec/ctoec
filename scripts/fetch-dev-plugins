#!/bin/sh
set -ex

rm -Rf wordpress/wp-content/themes/ctoec
git clone git@github.com:matthewcrist/benjamin.git wordpress/wp-content/themes/ctoec

rm -Rf wordpress/wp-content/plugins/forms-list
git clone git@github.com:ctoec/gutenberg-form-block.git wordpress/wp-content/plugins/forms-list

rm -Rf wordpress/wp-content/plugins/simple-alert
git clone git@github.com:ctoec/gutenburg-alert-block.git wordpress/wp-content/plugins/simple-alert